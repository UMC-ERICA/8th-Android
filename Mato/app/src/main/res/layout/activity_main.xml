<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/_____2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />


    <ImageButton
        android:id="@+id/yellowBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/group_462"
        android:layout_marginTop="228dp"
        android:layout_marginStart="162dp"
        android:padding="0dp"
        android:contentDescription="버튼"
        app:layout_constraintTop_toTopOf="@+id/back"
        app:layout_constraintStart_toStartOf="@+id/back"

        />

    <TextView
        android:layout_width="123dp"
        android:layout_height="21dp"
        android:text="더없이 행복한 하루였어요"
        android:gravity="center"
        android:textSize="18px"
        app:srcCompat="@drawable/group_462_1"
        android:layout_marginTop="278dp"
        android:layout_marginStart="121dp"
        app:layout_constraintTop_toTopOf="@+id/back"
        app:layout_constraintStart_toStartOf="@+id/back"/>


    <ImageView
        android:id="@+id/blue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/group_462_1"
        android:layout_marginTop="318dp"
        android:layout_marginStart="162dp"
        app:layout_constraintTop_toTopOf="@+id/back"
        app:layout_constraintStart_toStartOf="@+id/back" />

    <TextView
        android:layout_width="73dp"
        android:layout_height="21dp"
        android:text="들뜨고 흥분돼요"
        android:gravity="center"
        android:textSize="18px"
        app:srcCompat="@drawable/group_462_1"
        android:layout_marginTop="369dp"
        android:layout_marginStart="146dp"
        app:layout_constraintTop_toTopOf="@+id/back"
        app:layout_constraintStart_toStartOf="@+id/back"/>

    <TextView
        android:layout_width="185dp"
        android:layout_height="21dp"
        android:layout_marginTop="125dp"
        android:layout_marginStart="88dp"
        android:text="오늘 하루는 어땠나요"
        android:textAlignment="center"
        android:gravity="center"
        android:textSize="30px"
        app:layout_constraintTop_toTopOf="@+id/back"
        app:layout_constraintStart_toStartOf="@+id/back" />

    <TextView
        android:layout_width="147dp"
        android:layout_height="25dp"
        android:layout_marginTop="136dp"
        android:layout_marginStart="107dp"
