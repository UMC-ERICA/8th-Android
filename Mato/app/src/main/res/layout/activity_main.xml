        android:textAlignment="center"
        android:gravity="center"
        android:textSize="8px"
        app:layout_constraintTop_toTopOf="@+id/back"
        app:layout_constraintStart_toStartOf="@+id/back" />


    <ImageView
        android:id="@+id/purple"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/group_462_2"
        android:layout_marginTop="409dp"
        android:layout_marginStart="162dp"
        app:layout_constraintTop_toTopOf="@+id/back"
        app:layout_constraintStart_toStartOf="@+id/back" />

    <TextView
        android:layout_width="87dp"
        android:layout_height="21dp"
        android:text="평범한 하루였어요"
        android:gravity="center"
        android:textSize="18px"
        app:srcCompat="@drawable/group_462_1"
        android:layout_marginTop="460dp"
        android:layout_marginStart="139dp"
        app:layout_constraintTop_toTopOf="@+id/back"
        app:layout_constraintStart_toStartOf="@+id/back"/>


    <ImageView
        android:id="@+id/red"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/group_462_4"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="128dp"
        android:layout_marginTop="591dp"
        android:layout_marginStart="162dp"
        app:layout_constraintTop_toTopOf="@+id/back"
        app:layout_constraintStart_toStartOf="@+id/back" />

    <TextView
        android:layout_width="90dp"
        android:layout_height="21dp"
        android:text="부글부글 화가 나요"
