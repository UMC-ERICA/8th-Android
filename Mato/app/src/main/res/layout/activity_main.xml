        android:textSize="18px"
        app:srcCompat="@drawable/group_462_1"
        android:layout_marginTop="642dp"
        android:layout_marginStart="136dp"
        app:layout_constraintTop_toTopOf="@+id/back"
        app:layout_constraintStart_toStartOf="@+id/back"/>


    <ImageView
        android:id="@+id/green"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/group_462_3"
        android:layout_marginTop="500dp"
        android:layout_marginStart="162dp"
        app:layout_constraintTop_toTopOf="@+id/back"
        app:layout_constraintStart_toStartOf="@+id/back" />

    <TextView
        android:layout_width="127dp"
        android:layout_height="21dp"
        android:text="생각이 많아지고 불안해요"
        android:gravity="center"
        android:textSize="18px"
        app:srcCompat="@drawable/group_462_1"
        android:layout_marginTop="551dp"
        android:layout_marginStart="117dp"
        app:layout_constraintTop_toTopOf="@+id/back"
        app:layout_constraintStart_toStartOf="@+id/back"/>

    <ImageView
        android:id="@+id/vector"
        android:layout_width="8.06dp"
        android:layout_height="14.07dp"
        app:srcCompat="@drawable/vector"
        android:layout_marginTop="26dp"
        android:layout_marginStart="17dp"
        app:layout_constraintTop_toTopOf="@+id/back"
        app:layout_constraintStart_toStartOf="@+id/back" />

</androidx.constraintlayout.widget.ConstraintLayout>